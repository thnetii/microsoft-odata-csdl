{
  "name": "@thnetii/microsoft-odata-csdl-workspace-root",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thnetii/microsoft-odata-csdl.git"
  },
  "engines": {
    "node": ">=16"
  },
  "workspaces": [
    ".github/actions/*",
    ".github/lib/*"
  ],
  "scripts": {
    "lint": "npm-run-all --continue-on-error lint:*",
    "lint:tsc": "tsc --noEmit -p ./jsconfig.json",
    "lint:eslint": "eslint -c ./.eslintrc.yml .",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check ."
  },
  "devDependencies": {
    "@thnetii/eslint-config-node-base": "https://gitpkg.now.sh/thnetii/eslint-config/src/eslint-config-node-base?8425c1a",
    "@tsconfig/node16": "^16",
    "@tsconfig/strictest": "^2",
    "eslint": "^8.48.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8",
    "typescript": "^5"
  }
}
